  "private": true,
  "dependencies": {
    "bootstrap": "^3.3.7",
    "history": "^4.7.2",
    "jquery": "^3.2.1",
    "moment": "^2.20.1",
    "node-sass": "^3.4.2",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.1",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.0.0",
    "react-facebook-login": "^3.7.2",
    "react-google-login": "^3.0.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-scripts": "1.0.14",
    "react-select": "^1.0.0-rc.10",
    "react-share": "^1.18.0",
